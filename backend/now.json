{
    "version": 2,
    "name": "nodejs-express",
    "builds": [
        { "src": "index.js", "use": "@now/node-server" }
    ],
    "routes": [
        { "src": "/(.*)", "dest": "index.js" }
    ],
    "env": {
      "BING_MAP_API_KEY": "@bingmapkey",
      "IPSTACK_API_KEY": "@ipstack_api_key"
    }
}
